  "description": "View IdleLands Global information",
  "repository": "https://github.com/IdleLands/Global",
  "license": "MIT",
  "scripts": {
    "build:app": "webpack -p --progress && cp src/index.html dist/index.html && echo 'global.idle.land' > dist/CNAME && echo `date` > dist/builtat",
